    "drizzle-kit": "^0.31.9",
    "eslint": "^9",
    "eslint-config-next": "^16.1.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.5.3",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
